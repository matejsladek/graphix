{
  "name": "graphix",
  "version": "0.0.1",
  "description": "graphs",
  "main": "./dist/index.js",
  "scripts": {
    "build": "./node_modules/.bin/babel -d dist src/",
    "test": "exit 0",
    "prepublish": "npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matejsladek/graphix"
  },
  "binary": {
    "module_name" : "graphix",
    "module_path" : "./lib/binding/{configuration}/{node_abi}-{platform}-{arch}/",
    "remote_path" : "./{module_name}/v{version}/{configuration}/",
    "package_name": "{module_name}-v{version}-{node_abi}-{platform}-{arch}.tar.gz",
    "host"        : "https://node-pre-gyp-tests.s3-us-west-1.amazonaws.com"
  },
  "keywords": [
    "es6",
    "graphs"
  ],
  "author": "Matej Sladek <matejsladek10@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/matejsladek/graphix/issues"
  },
  "homepage": "https://github.com/matejsladek/graphix#readme",
  "dependencies": {
    "node-pre-gyp": "^0.6.36"
  },
  "bundledDependencies":["node-pre-gyp"],
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1"
  },
  "gypfile": true
}
